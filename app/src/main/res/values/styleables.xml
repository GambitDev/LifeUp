<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <declare-styleable name="TaskProgressBar">
        <attr name="bronzeMedalPosition" format="float"/>
        <attr name="silverMedalPosition" format="float"/>
        <attr name="progress" format="integer"/>
        <attr name="maxProgress" format="integer"/>
    </declare-styleable>

    <declare-styleable name="TaskView">
        <attr name="taskTitle" format="string"/>
    </declare-styleable>

    <declare-styleable name="CounterView">
        <attr name="counter" format="integer"/>
        <attr name="labelColor" format="color"/>
        <attr name="textColor" format="color"/>
        <attr name="progressColor" format="color"/>
        <attr name="progressWidth" format="dimension"/>
        <attr name="counterTextSize" format="dimension"/>
    </declare-styleable>
    
</resources>